@page
@model SRAUMOAR.Pages.generales.municipio.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Municipio[0].NombreMunicipio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Municipio[0].Distrito)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Municipio) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreMunicipio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Distrito.NombreDistrito)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.MunicipioId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.MunicipioId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.MunicipioId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
